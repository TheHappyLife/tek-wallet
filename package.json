{
  "name": "tek-wallet",
  "version": "0.0.420",
  "description": "A custom React provider with TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run format && tsc",
    "format": "prettier --write . && eslint --fix --ignore-pattern .next --ignore-pattern node_modules",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "prepublish": "npm run clean && npm run build",
    "publish:patch": "npm run version:patch && npm publish",
    "publish:minor": "npm run version:minor && npm publish",
    "publish:major": "npm run version:major && npm publish",
    "release": "git add . && git commit -m \"new version\" &&  npm run clean && npm run build && npm version patch"
  },
  "keywords": [
    "react",
    "auth",
    "authentication",
    "authjs"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/lab": "^7.0.0-beta.11",
    "@mui/material": "^6.4.7",
    "@mui/system": "^7.0.2",
    "@yudiel/react-qr-scanner": "^2.2.1",
    "ably": "^2.7.0",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.5",
    "framer-motion": "^12.7.4",
    "highcharts": "^12.2.0",
    "highcharts-react-official": "^3.2.1",
    "html-to-image": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.2.4",
    "notistack": "^3.0.2",
    "qrcode.react": "^4.2.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-otp-input": "^3.1.1",
    "react-qr-reader": "^3.0.0-beta-1",
    "styled-components": "^6.1.17",
    "swiper": "^11.2.4",
    "ton-core": "^0.53.0",
    "ton-crypto": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
